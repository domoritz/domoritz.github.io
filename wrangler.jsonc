{
  "name": "my-worker",
  "compatibility_date": "2025-08-01",
  "placement": {
    "mode": "smart"
  },
  "assets": {
    "directory": "_site",
    "html_handling": "auto-trailing-slash",
    "not_found_handling": "404-page"
  },
  "build": {
    "command": "jekyll build && npx @divriots/jampack _site"
  },
  "routes": [
    {
      "pattern": "www.domoritz.de",
      "custom_domain": true
    }
  ]
}
